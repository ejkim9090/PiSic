<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="FaqBoard">


<select id="pageSelectFaq" resultType="FaqBoard">
	SELECT * FROM faq
	ORDER BY faq_no DESC
</select>
<select id="selectFaqBoard" resultType="FaqBoard" parameterType="string">
	SELECT * FROM faq
	WHERE faq_no=#{faq_no}
</select>
<update id="qnaBoardCnt" parameterType="int">
	UPDATE faq
	SET
	faq_cnt = faq_cnt + 1
	WHERE
	faq_no = ${faq_no}
</update>


</mapper>
