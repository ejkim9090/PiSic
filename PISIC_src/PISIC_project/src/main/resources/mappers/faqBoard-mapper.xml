<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="FaqBoard">


<select id="pageSelectFaq" resultType="FaqBoard">
	   	    select * from faq
	order by faq_no desc
</select>
<select id="selectFaqBoard" resultType="FaqBoard" parameterType="string">
	select * from faq
	where faq_no=#{faq_no}
</select>

</mapper>
